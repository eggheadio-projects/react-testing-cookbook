    "redux": "3.6.0"
  },
  "devDependencies": {
    "babel-core": "6.16.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.16.0",
    "css-loader": "0.25.0",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "file-loader": "0.9.0",
    "mocha": "3.1.0",
    "parallelshell": "2.0.0",
    "react-addons-test-utils": "15.3.2",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
